{
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "monolog/monolog": "^2.8",
        "php-di/php-di": "^6.4",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.10"
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "^1.0.0",
        "laravel/pint": "^1.22",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/extension-installer": "^1.2.0",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5.26"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
